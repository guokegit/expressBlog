<%- include header %>
<% blogArr.forEach(function (blog) { %>
<div style="margin-top: 20px;padding:20px;background-color:#ffffff;border: #dddddd 1px solid;box-shadow: #dddddd 1px 1px 1px 1px;">
    <div class="flex-row">
        <a style="font-size: 25px"
           href="/u/<%= blog.name %>/<%= blog.time.day %>/<%= blog.title %>"><%= blog.title %></a>
        <div>
            <img src="/uploads/eye.png" style="margin-right: 5px;opacity: 0.6">
            <span style="font-size: 12px;color: #aaaaaa"><%= blog.pv %></span>
        </div>
    </div>

    <div style="font-size: 12px;margin-top: 20px" class="flex-row">
        <span style="font-size: 12px;color: #aaaaaa;"><%= blog.time.minute %></span>
        <div>
            <img src="/uploads/comment.png" style="margin-right: 5px;opacity: 0.6">
            <span style="color: #aaaaaa"><%= blog.commits.length %></span>
        </div>
    </div>
</div>
<% }) %>
<%- include changePage %>
<%- include footer %>