<%-include header%>
    <%blogArr.forEach(function (blog) {%>
        <div style="display: flex;flex-direction: row;justify-content: space-between;align-items: center;height: 60px;background-color: #ffffff;box-shadow: 2px 2px 2px #dddddd;margin-top: 20px;padding-left: 20px;padding-right: 20px">
            <p style="font-size: 20px;color: #333333;">
                <a href="/u/<%=blog.name%>/<%=blog.time.day%>/<%=blog.title%>">
                    <%=blog.title%>
                </a>
            </p>
            <span style="font-size: 12px;color: #aaaaaa"><%=blog.time.minute%></span>
        </div>
    <%})%>
<%-include footer%>